{
  "name": "auth0-nextjs-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"node api-server\"",
    "dev:api": "nodemon api-server",
    "build": "next build",
    "start": "concurrently \"next start\" \"node api-server\"",
    "start:api": "node api-server",
    "test": "jest tests --coverage --maxWorkers=10",
    "test:watch": "jest --coverage --watch",
    "test:integration": "start-server-and-test start http-get://localhost:3000 cypress:run",
    "test:integration:watch": "start-server-and-test start http-get://localhost:3000 cypress:open",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.0.0",
    "@editorjs/code": "^2.8.0",
    "@editorjs/editorjs": "^2.27.2",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/link-autocomplete": "^0.1.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/nested-list": "^1.3.0",
    "@editorjs/paragraph": "^2.9.0",
    "@editorjs/text-variant-tune": "^1.0.1",
    "@editorjs/underline": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^1.0.6",
    "@next/font": "^13.4.5",
    "@supabase/supabase-js": "^2.26.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "@typeform/embed-react": "^2.28.0",
    "cloudinary-react": "^1.8.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "editorjs-drag-drop": "^1.1.13",
    "editorjs-undo": "^2.0.22",
    "express": "^4.18.2",
    "express-jwt": "^8.0",
    "framer-motion": "^10.12.22",
    "helmet": "^6.0.1",
    "highlight.js": "^11.7.0",
    "jwks-rsa": "^3.0.0",
    "lottie-react": "^2.4.0",
    "marked": "^5.1.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "next": "^13.1.0",
    "nodemon": "^2.0.20",
    "openai": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "reactstrap": "^9.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.3.1",
    "cypress": "^11.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.26",
    "prettier": "^2.8.0",
    "start-server-and-test": "^1.15.1",
    "tailwindcss": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "rootDir": "./",
    "moduleNameMapper": {
      ".(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./tests/environment.js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "components/**/*.*",
      "pages/**/*.*"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
